{
  "name": "acsset",
  "version": "0.2.2",
  "description": "ACSSET (A CSS Set) Ã© um framework front-end leve, moderno e responsivo.",
  "main": "css/acsset.min.css",
  "dependencies": {},
  "devDependencies": {
    "node-sass": "^3.10.0"
  },
  "scripts": {
    "reset": "rm -rf css",
    "sass": "mkdirp css && node-sass --output-style compressed sass/acsset.scss > css/acsset.min.css && node-sass --output-style compact sass/acsset.scss > css/acsset.css",
    "sass:watch": "node-sass -w sass/acsset.scss -o css/",
    "acsset:build": "npm run sass",
    "postinstall": "npm run acsset:build"
  },
  "keywords": [
    "css",
    "sass",
    "framework",
    "front-end",
    "responsivo",
    "helper",
    "acsset",
    "boilerplate",
    "starter kit",
    "template"
  ],
